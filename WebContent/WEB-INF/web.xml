<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>cn.sqat.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>TelegramServlet</display-name>
		<servlet-name>TelegramServlet</servlet-name>
		<servlet-class>cn.sqat.controller.TelegramServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TelegramServlet</servlet-name>
		<url-pattern>/TelegramServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>LogoutServlet</display-name>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>cn.sqat.controller.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>SalesServlet</display-name>
		<servlet-name>SalesServlet</servlet-name>
		<servlet-class>cn.sqat.controller.SalesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SalesServlet</servlet-name>
		<url-pattern>/SalesServlet</url-pattern>
	</servlet-mapping>
		<servlet>
		<display-name>ReportServlet</display-name>
		<servlet-name>ReportServlet</servlet-name>
		<servlet-class>cn.sqat.controller.ReportServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReportServlet</servlet-name>
		<url-pattern>/ReportServlet</url-pattern>
	</servlet-mapping>
</web-app>